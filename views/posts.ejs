<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('partials/head.ejs') %>
</head>
<body>
  <%- include('partials/header.ejs') %>
  
  <section>
    <h1>Posts</h1>

    <article>
      <% posts.forEach((item) => { %>
        <div>
          <h2><%= item.title %></h2>
          <p><%= item.content %></p>
        </div>
      <% }) %>
    </div>
  </section>

  <%- include('partials/footer.ejs') %>
  <%- include('partials/foot.ejs') %>
</body>
</html>